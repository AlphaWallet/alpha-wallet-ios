                <xhtml:script type="text/javascript">//
class Token {

    constructor(token, card) {
        this.token = token;
        this.card = card;
    }

    render() {
        const decimals18 = 1e+18;
        let totalBorrowsWithInterest = this.token.borrowBalance / decimals18;
        let interestAccrued = (this.token.borrowBalance - this.token.borrowBalancePrincipal) / decimals18;
        let principal = this.token.borrowBalancePrincipal / decimals18;
        return`
        &lt;div class="repay action"&gt;
           &lt;h1&gt;Repay&lt;/h1&gt;
           &lt;h2&gt;&lt;img src="data:image/png;base64,"&gt; DAI&lt;/h2&gt;
           &lt;input id="repayAmount" type="number" placeholder="0"&gt;
           &lt;p class="hint"&gt; Remaining to Repay: ${totalBorrowsWithInterest.toFixed(4)} DAI&lt;/p&gt;
           &lt;p class="repayAll" onclick="(function () {
                document.getElementById('repayAmount').value = ${totalBorrowsWithInterest}
            })();"&gt;Repay all&lt;/p&gt;
           &lt;table&gt;
              &lt;caption&gt;Details&lt;/caption&gt;
              &lt;tbody&gt;
                 &lt;tr&gt;
                    &lt;th&gt;Total to repay &lt;/th&gt;
                    &lt;td&gt;${totalBorrowsWithInterest.toFixed(2)}&lt;/td&gt;
                 &lt;/tr&gt;
                 &lt;tr&gt;
                    &lt;th&gt;Borrowed&lt;/th&gt;
                    &lt;td&gt;${(principal).toFixed(2)} DAI&lt;/td&gt;
                 &lt;/tr&gt;
                 &lt;tr&gt;
                    &lt;th&gt;Interest to Repay&lt;/th&gt;
                    &lt;td&gt;${interestAccrued.toFixed(7)} DAI&lt;/td&gt;
                 &lt;/tr&gt;
              &lt;/tbody&gt;
           &lt;/table&gt;
        &lt;/div&gt;
`;
    }
}

web3.tokens.dataChanged = (oldTokens, updated, tokenIdCard) =&gt; {
    document.getElementById(tokenIdCard).innerHTML = new Token(updated.token, updated.card).render();
};
//
